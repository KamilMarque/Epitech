#include "my.h"

char	*my_strcat(char *dest, char *src)
{
  int	i;
  inti	j;

  j = 0;
  i = my_strlen(dest);
  while (src[j])
    dest[i++] = src[j++];
  dest[i] = '\0';
  return (dest);
}
